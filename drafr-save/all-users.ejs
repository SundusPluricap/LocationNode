<%- include('../partials/header') -%>
<title>Location, Users</title>
<link rel="stylesheet" href="/css/user.css">
</head>
<body>
    <div class="container">
        <%- include('../partials/sidebar') -%>
        <div class="main-content">
            <h1>All Users</h1>
            <table class="user-table">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Role</th>
                    
                </tr>
                </thead>
                <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.id %></td>
                        <td><%= user.firstName %></td>
                        <td><%= user.lastName %></td>
                        <td><%= user.email %></td>
                        <td><%= user.role %></td>
                    </tr>
                <% }) %>
                </tbody>
            </table>
        </div>
    </div>


<%- include('../partials/footer') -%>


    
</body>
</html>
